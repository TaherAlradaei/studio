
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    
    // WARNING: These rules are completely public and insecure.
    // Anyone can read, write, and delete any data in your database.
    // This is for development and debugging purposes only.
    // You should secure your data before launching your application.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
